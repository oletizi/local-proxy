{
  "name": "local-proxy",
  "version": "1.0.0",
  "description": "A local network proxy to mediate web requests and log transactions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["proxy", "network", "logging", "typescript"],
  "author": "",
  "license": "MIT",
  "bin": {
    "proxyctl": "dist/cli.js"
  },
  "dependencies": {
    "http-proxy-middleware": "^2.0.6",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}